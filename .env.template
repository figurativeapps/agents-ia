# API Keys Configuration
# Copy this file to .env and fill in your actual API keys

# Google Maps / Serper API (for business search & OSINT)
SERPER_API_KEY=your_serper_api_key_here

# Firecrawl (for website qualification and scraping)
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Hunter.io (for email pattern detection - optional but recommended)
HUNTER_API_KEY=your_hunter_api_key_here

# HubSpot CRM (for contact and company sync)
HUBSPOT_API_KEY=your_hubspot_api_key_here

# Apify (for Google Maps reviews - optional)
APIFY_API_KEY=your_apify_api_key_here


